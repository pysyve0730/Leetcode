char* reverseString(char* s, int sSize) {
    int i = 0;
    char temp;
    while (i < sSize/2) {
        temp = *(s + i);
        *(s + i) = *(s + sSize - 1 - i);
        *(s + sSize - 1 - i) = temp;
        i++;


    }

   
    for (i = 0; i < sSize; i++) {
        printf("%c\n", *(s + i));
    }
    return s;





}
