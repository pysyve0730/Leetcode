//iterative
int searchInsert(int* nums, int numsSize, int target){
   
    int i = 0;
    int start = 0;
    int end = numsSize-1;
    while(start<=end){
        int middle = (start+end)/2;
        if(nums[middle]==target){
            return middle;
        }
        if(nums[middle]>target){
            end=middle-1;
        }
        if(nums[middle]<target){
            start=middle+1;
            
        }   
        
    }
    return start;

    
    
}
