//recursive
int fib(int n){
    if(n==0){
        return 0;
    }
    if(n==1){
        return 1;
    }   
    return fib(n-1)+fib(n-2);

}


//Iterative (overflow)
int fib(int n){
    int *arr = (int*)malloc(sizeof(int)*(n+1));
    int i = 0;
    int sum = 0;
    arr[0] = 0;
    arr[1] = 1;
    
    for(i=2;i<=n;i++){
       arr[i] = arr[i-1]+arr[i-2];
    }
    
    return arr[n];

}



