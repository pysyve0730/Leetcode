struct TreeNode* sub(struct TreeNode* root,int* nums, int numsSize){
    

}





struct TreeNode* constructMaximumBinaryTree(int* nums, int numsSize){
    struct TreeNode *root = (struct TreeNode*)malloc(sizeof(struct TreeNode));
    int i = 0;
    int max = 0;
    for(i=0;i<numsSize;i++){
        if(nums[i]>max){
            max = nums[i];
        }
    }
    root->val = max;
    // printf("%d",root->val);
    sub(root,nums,numsSize);
    
    return root;
}
