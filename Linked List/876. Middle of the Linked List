struct ListNode* middleNode(struct ListNode* head){
    int size = 0;
    struct ListNode* curr = head;
    //先計算linked list的長度
    while(head!=NULL){
        head = head->next;
        size++;  
    }
    //找出中間值
    int middle = size/2;
    
    //從中間值到結尾
    while(middle--){
         curr = curr->next;

    }
    return curr;
    

}
