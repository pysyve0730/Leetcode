#include <stdio.h>
#include <stdlib.h>





int majorityElement(int* nums, int numsSize) {

        int i = 0, j = 0;
        int count = 0;
        for (i = 0; i < numsSize; i++) {
            if (count == 0) {
                j = nums[i];
            }

            if (nums[i] == j) {
                count++;//注意count變化
            }
            else {
                count--;
            }

        }
        return j;


    }



void main() {
    int nums[] = { 2,2,1,1,1,2,2 };
    int numsSize = sizeof(nums) / sizeof(nums[0]);
    majorityElement(nums,numsSize);





}
